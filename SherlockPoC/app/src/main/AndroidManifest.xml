<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="sherlock.test.NORMAL_PERMISSION"/>
    <uses-permission android:name="sherlock.test.DANGEROUS_PERMISSION"/>

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/Theme.SherlockPoC">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".insecure_set_result.BasicActivity"/>
        <activity android:name=".insecure_set_result.ImplicitIntentActivity"/>
        <activity
            android:name=".insecure_set_result.InterceptImplicitIntentActivity"
            android:exported="true">
            <intent-filter android:priority="999">
                <action android:name="sherlock.test.INSECURE_SETRESULT_ACTION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".insecure_set_result.DeeplinkActivity"/>
        <activity android:name=".intent_redirection.BasicActivity"/>
        <activity android:name=".intent_redirection.BasicDeeplinkActivity"/>
        <activity android:name=".intent_redirection.ImplicitIntentActivity"/>
        <activity android:name=".intent_redirection.DeeplinkActivity"/>
        <activity
            android:name=".intent_redirection.InterceptImplicitIntentActivity"
            android:exported="true">
            <intent-filter android:priority="999">
                <action android:name="sherlock.test.INTENT_REDIRECT_ACTION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".unsafe_intent_uri.component.BasicActivity"/>
        <activity android:name=".unsafe_intent_uri.component.ImplicitIntentActivity"/>
        <activity
            android:name=".unsafe_intent_uri.component.InterceptImplicitIntentActivity"
            android:exported="true">
            <intent-filter android:priority="999">
                <action android:name="sherlock.test.UNSAFE_INTENT_URI_BASIC"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".unsafe_intent_uri.webview.BasicActivity"/>
        <activity android:name=".unsafe_intent_uri.webview.BasicDeeplinkActivity"/>
        <activity android:name=".unsafe_intent_uri.webview.ImplicitIntentActivity"/>
        <activity android:name=".unsafe_intent_uri.webview.DeeplinkActivity"/>
        <activity
            android:name=".unsafe_intent_uri.webview.InterceptImplicitIntentActivity"
            android:exported="true">
            <intent-filter android:priority="999">
                <action android:name="sherlock.test.UNSAFE_INTENT_URI_WEBVIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".mutable_pending_intent.LeakProviderActivity"
            android:exported="true">
            <intent-filter android:priority="999">
                <action android:name="sherlock.poc.TARGET_ACTIVITY"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".mutable_pending_intent.InterceptBaseActivity"
            android:exported="true">
            <intent-filter android:priority="999">
                <action android:name="sherlock.test.IMPLICIT_BASE_INTENT"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".mutable_pending_intent.HijackEmptyBaseIntentActivity"
            android:exported="true">
            <intent-filter android:priority="999">
                <action android:name="sherlock.test.MUTABLE_PENDING_INTENT_EMPTY"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".mutable_pending_intent.HijackImplicitBaseIntentActivity"
            android:exported="true">
            <intent-filter android:priority="999">
                <action android:name="sherlock.test.MUTABLE_PENDING_INTENT_IMPLICIT"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".mutable_pending_intent.InterceptBroadcastActivity"
            android:exported="true"/>
        <activity
            android:name=".mutable_pending_intent.InterceptNotifBaseActivity"
            android:exported="true">
            <intent-filter android:priority="999">
                <action android:name="sherlock.test.NOTIFICATION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".insecure_broadcast.BasicActivity"/>
        <activity android:name=".insecure_broadcast.InsufficientProtectionActivity"/>
        <activity
            android:name=".intent_redirection.LeakProviderActivity"
            android:exported="true"/>
        <service
            android:name=".mutable_pending_intent.NotifListenerService"
            android:exported="true"
            android:permission="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE">
            <intent-filter>
                <action android:name="android.service.notification.NotificationListenerService"/>
            </intent-filter>
        </service>
    </application>

</manifest>